<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
         "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
    <rule>
        <from>^/matiere\?id=(\d+)&\?nom=(\w+)$</from>
        <to>/matiere/{id}/{nom}</to>
        <set name="id">$1</set>
        <set name="nom">$2</set>
    </rule>
    <rule>
        <from>/page</from>
        <to>/pageLoginAdmin</to>
        
    </rule>
    
</urlrewrite>